{
	"compilerOptions": {
		"lib": [
			"ES2022"
		],
		"module": "Node16",
		"noFallthroughCasesInSwitch": true,
		"noUnusedParameters": true,
		"outDir": "out",
		"rootDir": "src",
		"sourceMap": true,
		"strict": true,
		"target": "ES2022",
		/* Additional Checks */
		"noImplicitReturns": true, /* Report error when not all code paths in function return a value. */
	},
	"exclude": ["ui", "node_modules", ".vscode-test"]
}
